{
  "name": "jalikoa/binancepay",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "nikic/fast-route": "^1.3",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/database": "^10.0",
    "vlucas/phpdotenv": "^5.5",
    "monolog/monolog": "^3.0",
    "symfony/http-foundation": "^6.0",
    "respect/validation": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "chmod -R 775 storage"
    ],
    "post-update-cmd": [
      "chmod -R 775 storage"
    ]
  }
}